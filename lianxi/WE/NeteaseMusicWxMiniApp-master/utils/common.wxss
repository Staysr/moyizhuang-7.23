.rm_header{font-size: 24rpx;color: #999;line-height:1.8;padding-bottom: 10rpx;}
.rm_header view{float: right;line-height: 40rpx;}
.fmh_time{display: block;font-size: 18rpx;}
.rm_header .fmh_name{font-size: 28rpx;color: #333;display:block}
