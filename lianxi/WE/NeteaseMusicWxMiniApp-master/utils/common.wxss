@keyframes circle{
	0%{transform: rotate(0deg)}
	100%{ transform: rotate(360deg)}
}
@keyframes cnloading{
	0%{ transform: translateY(0rpx)}
	50%{ transform: translateY(20rpx)}
	100%{ transform: translateY(0rpx)}
}
.ml{margin-left: 20rpx}
.fr{float: right}
.fc_gray{color: #949597}
#playingpage{overflow: hidden;height: 100%;position: relative;color: #fff}
.st_title{overflow: hidden;margin: 10rpx 15rpx 0 15rpx;padding: 10rpx;padding-left: 0;line-height: 36rpx;font-size:28rpx;}
.st_title image{width:36rpx;height: auto;margin-right: .3em;vertical-align: middle;font-size:28rpx;}
.st_title .rbtn{float: right;color: #999;font-size: 24rpx;}
.blurbg{
    filter: blur(30px) brightness(.8) ;
	background-size: cover;
	background-position: center center;
	background-repeat: no-repeat;
	position: absolute;
	width: 100%;
	height: 100%;
	z-index: -1;
	left: 0;
	top: 0;
	transform: scale(1.2);background-color: transparent
}
 
.cntloading{padding: 50rpx 0;text-align: center;color: #888;font-size: 26rpx;}
.cntloading view{display: inline-block;vertical-align: baseline;height: 30rpx;overflow: hidden;margin-left: 10rpx;}
.cntloading text{width: 5rpx;height: 30rpx;background-color: #d33a31;position: relative;display: inline-block;margin-right: 6rpx;
border-radius: 3rpx;animation: cnloading .9s linear infinite; transform-origin: center bottom;}
.cntloading .cl2{ animation-delay: -.6s}
.cntloading .cl3{animation-delay: -.3s}

 
#playing-status{display: flex;color:#fff;margin: 20rpx}
#playing-status progress{ width:520rpx;margin: 0 auto;color: #000;position: relative;border-radius: 4rpx;}
#playing-status #playstate{position: absolute;display: block;height:8rpx;width: 8rpx;overflow: hidden;background-color: #d33a31;
border:12rpx solid #fff;border-radius: 50%;top:50%; transform: translate3d(-50%,-50%,0);z-index: 2}
#dpstate{position: absolute;height:2px;width: 0;background-color:#d33a31;overflow: hidden;left: 0;top:50%;margin-top: -1px;}
#playing-status .time{width:80rpx ;text-align:center;display: block;font-size: 24rpx;}
#playing-status slider{position: absolute;left: 0;top:0;width: 100%;margin: 0;opacity: 0;z-index: 10}

.tab{display: flex;border-bottom: 1rpx solid rgba(200,200,200,.5); background:rgba(240,240,240,.8);height: 80rpx;line-height: 80rpx;}
.tab .tab-item{text-align: center; flex:auto; word-break: keep-all;font-size: 30rpx;}
.tab .tab-item .tbi-text{display: inline-block;padding: 0 .3em; vertical-align: top;line-height: 76rpx;height: 77rpx;}
.tab .tab-item .tbi-des{display: inline;color: #999; border-bottom: none;font-size: 24rpx;line-height: 1;padding-left: .5em}
.tab .tbi-cur .tbi-text{color: #d33a31; margin-bottom: -1rpx;border-bottom:4rpx solid  #d33a31}

.ptab{display: flex;margin: 0 auto;background: #BB2C08; justify-content: center;padding: 0 0 20rpx;width: 100%}
.ptab .pt-item{ text-align: center;line-height: 2em;padding: 0 1em;color: #fff;border: 1rpx solid #fff;margin-left: 1rpx;border-radius: 4rpx 0 0 4rpx;width: 4em;margin-left: -1rpx;font-size: 28rpx;}
.ptab .pt-item:last-child{border-radius: 0 4rpx 4rpx 0;}
.ptab .pt_cur{color: #BB2C08;border-color: #fff;background: #fff}

.rec_list{display: flex;padding: 20rpx 0 0 20rpx;}
.rec_avatar,.rec_avatar image{width: 60rpx;height: 60rpx;border-radius: 50%;}
.rec_main{border-bottom: 1rpx solid #e1e2e3;margin-left: 20rpx;width:650rpx;font-size: 28rpx;padding: 0 20rpx 15rpx 0;line-height: 48rpx;}
