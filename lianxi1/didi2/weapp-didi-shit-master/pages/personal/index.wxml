<!--pages/personal/index.wxml-->
<view class="content">
      <view class = "logo">
        <open-data type="userAvatarUrl" class="open"></open-data>
    </view>
    <view class='name'>
    <text>{{username}}</text>
    </view>
    <view>
     <view class='img' bindtap='dingdan'>
        <image mode="aspectFit" src="../../images/my-dingdan.png"></image>
        <text class='text1'>我的订单</text>
     </view> 
     <view class='img' bindtap='jifen'>
        <image mode="aspectFit" src="../../images/my-jifen.png"></image>
        <text class='text1'>我的积分</text>
     </view>
     <view class='img' bindtap='shangcehng'>
        <image mode="aspectFit" src="../../images/my-shopping.png"></image>
        <text class='text1'>积分商城</text>
     </view>
     <view class='img' bindtap='kefu'>
        <image mode="aspectFit" src="../../images/my-kefu.png"></image>
        <text class='text1'>客服中心</text>
     </view>
     <view class='img' bindtap='shezhi'>
        <image mode="aspectFit" src="../../images/my-shezhi.png"></image>
        <text class='text1'>设置</text>
     </view>
    </view>
</view>
