

.open{
  width: 140rpx;
  height: 140rpx;
  border-radius: 50%;
  margin-top: 98rpx;
  display: block;
  overflow: hidden;
}

.car-Top{
  margin:0 auto;
  width:20%;
  margin-top:5%;
}
.car-top{
  width:140rpx;
  height:140rpx;
  text-align:center;
  margin:0 auto;
}
.car-top image{
  width:100%;
  height:100%;
  border-radius:50%;
}
.car-top span{
  font-size:16px;
  color:#000000;
}

.head{
    width:90%;
    margin:0 auto;
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
    align-items: center;
    margin-top:150rpx;
}
.head image{
  width:130rpx;
  height:130rpx;
}
.head-left{
	width:33%;
	text-align:center;
  margin-top:40rpx;
}

.head-left span{
  display:block;
  text-align:center;
  font-size:16px;
  color:#000000;
}
.head-middle{
	width:33%;
	text-align:center;
  margin-top:40rpx;
  position:relative;
}
.head-middle span{
  display:block;
  text-align:center;
  font-size:16px;
  color:#000000;
}
.head-right{
	width:33%;
	text-align:center;
  margin-top:40rpx;
}

.head-right span{
  display:block;
  text-align:center;
  font-size:16px;
  color:#000000;
}
.dx{
  position:absolute;
  top:20%;
  left:32%;
}
.wx-switch-input{
  width:42px !important;
  height:20px !important;
  border:none !important;
}
.wx-switch-input::before{
  width:42px !important;
  height: 20px !important;
  background:#ffffff !important;
  }
.wx-switch-input::after{
  width:20px !important;
  height:20px !important;
  background:#cccccc !important;
}

