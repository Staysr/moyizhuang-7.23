
image {
  display: block;
  width: 100%;
  height: 100%;
}
view {
  box-sizing: border-box;
}
.face {
  position: absolute;
  width: 100%;
  height: 100%;
  z-index: 999999;
  top: 0;
  left: 0;
  background: rgba(0,0,0,0.01);
  border: 0;
  margin: 0;
  padding: 0;
  border-radius: 0;
}
.none {
  position: absolute;
  width: 0rpx;
  height: 0rpx;
  z-index: -999999;
  overflow: hidden;
  background: rgba(0,0,0,0)
  border: 0;
  margin: 0;
  padding: 0;
  border-radius: 0;
}
.zone {
  position: absolute;
  width: 100%;
  height: 100%;
  z-index: 999999;
  top: 0;
  left: 0;
  background: rgba(0,0,0,0)
  border: 0;
  margin: 0;
  padding: 0;
  border-radius: 0;
}
