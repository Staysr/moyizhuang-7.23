.pro-info{padding: 30rpx; background-color: #fff; justify-content: space-between;}
.pro-info image{width: 195rpx; height: 195rpx;}
.pro-info .txt-box{width: 468rpx; flex-direction: column; justify-content: space-between;}
.pro-info .txt-box .title{display: -webkit-box; display:-moz-box; display:box;-webkit-box-orient: vertical; -moz-box-orient: vertical; -webkit-line-clamp: 2;-moz-line-clamp: 2;overflow: hidden;}
.pro-info .txt-box .count{color: #999999; font-size: 24rpx;}
.pro-info .txt-box .price{font-size: 42rpx; color: #ff3d3d; align-items: flex-end;}
.pro-info .txt-box .price text{font-size: 32rpx;}
.pro-info .txt-box .price .old-price{color: #999; font-size: 24rpx; margin:0 0 5rpx 10rpx; text-decoration:line-through;}
.con-wrapper{padding: 0rpx 20rpx 26rpx; margin-top: 28rpx; background-color: #fff;}
.con-wrapper .join-people{padding: 0 24rpx; flex-wrap: wrap;}
.con-wrapper .join-people .item{position: relative; height: 93rpx; width: 93rpx;  border-radius: 50%; margin-right: 50rpx; margin-top: 26rpx;}
.con-wrapper .join-people .item:nth-child(5n){margin-right: 0;}
.con-wrapper .join-people .item image{height: 93rpx; width: 93rpx;  border-radius: 50%; box-sizing: border-box;border: 1px solid #ff3d3d;}
.con-wrapper .join-people .item .txt-title{position: absolute; right: -17rpx; top: -13rpx; background-color: #ff3d3d; color: #fff; width: 72rpx; height: 30rpx; line-height: 30rpx; text-align: center; border-radius: 30px; font-size: 22rpx;}
.con-wrapper .join-people .item.unknown{border: 1px dashed #dcdcdc; border-radius: 50%; text-align: center; line-height: 93rpx;}
.con-wrapper .join-people .item.unknown image{border: 0; border-radius: 0; width: 25rpx; height: 44rpx; vertical-align: middle;}
.tips{align-items: center; justify-content: center; margin-top: 25rpx;}
.tips .number{color: #ff3d3d;}
.tips .out{margin-left: 10rpx; padding-left: 25rpx; background-image: url('data:image/png;base64,'); background-repeat: no-repeat; background-size: 21rpx 24rpx; background-position: left center;}

.cound-time{margin-top: 10rpx; justify-content: center; align-items: center;}
.sub-btn{margin-top: 30rpx; background-color: #ff3d3d; color: #fff; font-size: 32rpx;}
.status-txt{margin-top: 25rpx;}
.status-txt .success{color: #ff3d3d; justify-content: center; align-items: center;}
.status-txt .success .icon{width: 30rpx; height: 30rpx; background-image: url('data:image/png;base64,'); background-size: 100% 100%; margin-right: 15rpx;}
.fhz-txt{text-align: center;color: #666;}
.error{color: #666; justify-content: center; align-items: center;}
.error .icon{background-image: url('data:image/png;base64,'); width: 30rpx; height: 30rpx; background-size: 100% 100%; margin-right: 10rpx;}
.like-pro .title-bar{height: 130rpx; align-items: center; justify-content: center;}
.like-pro .title-bar .line{width: 202rpx; height: 1px; background-color: #999;}
.like-pro .title-bar .text{color: #666; margin: 0 16rpx;}
.like-pro .title-bar .text .iconfont{font-size: 30rpx; margin-right: 12rpx;}
.list-wrapper{padding: 0 10rpx; flex-wrap: wrap; justify-content: space-between;}
.list-wrapper .item{position: relative; width: 360rpx; background-color: #fff;}
.list-wrapper .item image{width: 100%; height: 360rpx;}
.list-wrapper .item .text-bar{padding: 15rpx;}
.list-wrapper .item .text-bar .title{font-size: 30rpx; overflow: hidden;white-space: nowrap;text-overflow: ellipsis;}
.list-wrapper .item .text-bar .count{color: #ababab; font-size: 22rpx; margin: 10rpx 0;}
.list-wrapper .item .text-bar .price{font-size: 32rpx; color: #ff3d3d; align-items: center;}
.list-wrapper .item .text-bar .price text{font-size: 22rpx;}
.list-wrapper .item .text-bar .price .small-txt{font-size: 24rpx; width: 35rpx; color: #fff; border-radius: 3px; height: 40rpx; line-height: 40rpx; text-align: center;margin-left: 10rpx;}
.list-wrapper .item .text-bar .price .jing{background-color: #ff3d3d;} 
.buy-car{position: absolute; right: 20rpx; bottom: 10rpx; width: 55rpx; height: 55rpx; line-height: 55rpx;text-align: center; border:1px solid #d81e06; color:#d81e06; border-radius: 50%; box-sizing: border-box;}